<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.homegym_remake">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Homegym_remake">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RecItemActivity"
            android:label="RecItemActivity 액티비티" />
        <activity
            android:name=".LrecItemActivity"
            android:label="LrecItemActivity 액티비티" />
        <activity
            android:name=".WrecItemActivity"
            android:label="WrecItemActivity 액티비티" />
        <activity
            android:name=".UpperActivity"
            android:label="UpperActivity 액티비티" />
        <activity
            android:name=".LowerActivity"
            android:label="LowerActivity 액티비티" />
        <activity
            android:name=".WholeBodyActivity"
            android:label="WholeBodyActivity 액티비티" />
        <activity
            android:name=".LoginActivity"
            android:label="LoginActivity 액티비티" />
    </application>
</manifest>